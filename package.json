{
  "name": "franken-wc",
  "author": "Sveltecult <leader@sveltecult.com>",
  "description": "Franken WC is a project designed to complement UIkit, filling in the gaps with the missing components of Franken UI.",
  "keywords": [
    "franken-ui",
    "web-components",
    "shadcn-ui",
    "uikit",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/franken-ui/wc.git"
  },
  "bugs": {
    "url": "https://github.com/franken-ui/wc/issues"
  },
  "homepage": "https://www.franken-ui.dev",
  "license": "MIT",
  "version": "0.0.6",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:js": "tsc && vite -c config/wc.js build",
    "build:icons": "tsc && vite -c config/icons.js build",
    "build:css": "vite -c config/css.js build",
    "prepublishOnly": "npm run build:js & npm run build:icons & npm run build:css",
    "preview": "vite preview"
  },
  "dependencies": {
    "franken-ui": "^0.1.3",
    "lit": "^3.1.4",
    "lucide": "^0.428.0",
    "postcss": "^8.4.39",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.4.4"
  },
  "devDependencies": {
    "@open-wc/eslint-config": "^12.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  },
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid",
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/js/wc.iife.js"
  }
}
